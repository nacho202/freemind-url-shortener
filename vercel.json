{
  "functions": {
    "api/links.js": { "runtime": "edge" },
    "api/redirect.js": { "runtime": "edge" },
    "api/styles.js": { "runtime": "edge" }
  },
  "routes": [
    { "src": "/api/links", "dest": "/api/links" },
    { "src": "/styles.css", "dest": "/api/styles" },
    { "src": "/(.*)", "dest": "/api/redirect?slug=$1" }
  ]
}
