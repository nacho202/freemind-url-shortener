{
  "buildCommand": "",
  "outputDirectory": ".",
  "installCommand": "npm install",
  "rewrites": [
    { "source": "/", "destination": "/api/simple-main" },
    { "source": "/original", "destination": "/api/index" },
    { "source": "/test", "destination": "/api/test-page" },
    { "source": "/test-fixed", "destination": "/api/test-fixed" },
    { "source": "/api/links", "destination": "/api/links" },
    { "source": "/api/history", "destination": "/api/history" },
    { "source": "/api/ultra-simple", "destination": "/api/ultra-simple-fixed" },
    { "source": "/api/edit-simple/(.*)", "destination": "/api/ultra-simple-fixed/$1" },
    { "source": "/api/delete-simple/(.*)", "destination": "/api/ultra-simple-fixed/$1" },
    { "source": "/api/simple-links", "destination": "/api/simple-links" },
    { "source": "/api/simple-history", "destination": "/api/simple-history" },
    { "source": "/api/edit/(.*)", "destination": "/api/edit/$1" },
    { "source": "/api/update/(.*)", "destination": "/api/update/$1" },
    { "source": "/api/stats/(.*)", "destination": "/api/stats/$1" },
    { "source": "/api/delete/(.*)", "destination": "/api/delete/$1" },
    { "source": "/api/test-kv", "destination": "/api/test-kv" },
    { "source": "/favicon.ico", "destination": "/api/favicon" },
    { "source": "/styles.css", "destination": "/api/styles" },
    { "source": "/(.*)", "destination": "/api/simple-redirect-v2?slug=$1" }
  ]
}
